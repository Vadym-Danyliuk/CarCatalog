{
  "name": "rental-car",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Car rental service with modern React architecture",
  "keywords": [
    "react",
    "car-rental",
    "vite",
    "css-modules"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
